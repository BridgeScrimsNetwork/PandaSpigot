+++ b/src/main/java/org/spigotmc/SpigotWorldConfig.java
@@ -29,7 +29,7 @@ public class SpigotWorldConfig
 
     private void log(String s)
     {
-        if ( verbose )
+        if ( false )
         {
             Bukkit.getLogger().info( s );
         }
